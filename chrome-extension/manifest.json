{
    "manifest_version": 3,
    "name": "ASIN Scraper Extension",
    "version": "1.0",
    "description": "A simple Chrome extension",
    "permissions": ["activeTab", "http://localhost:5000/"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
      }
    ]

  }
  